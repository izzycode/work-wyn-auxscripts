{
    "parserOptions": {
        "ecmaVersion": 9,
        "sourceType": "script",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "brace-style": 2,
        "camelcase": 2,
        "class-methods-use-this": 2,
        "consistent-return": 2,
        "curly": 2,
        "default-case": 2,
        "dot-notation": 2,
        "eqeqeq": 2,
        "for-direction": 2,
        "global-require": 2,
        "no-case-declarations": 2,
        "no-compare-neg-zero": 2,
        "no-cond-assign": 2,
        "no-const-assign": 2,
        "no-dupe-args": 2,
        "no-dupe-class-members": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-duplicate-imports": 2,
        "no-empty": 2,
        "no-empty-character-class": 2,
        "no-empty-function": 2,
        "no-extra-bind": 2,
        "no-fallthrough": 2,
        "no-iterator": 2,
        "no-lone-blocks": 2,
        "no-lonely-if": 2,
        "no-magic-numbers": 2,
        "no-mixed-operators": 2,
        "no-mixed-requires": 2,
        "no-mixed-spaces-and-tabs": 2,
        "no-multi-assign": 2,
        "no-multi-spaces": 2,
        "no-nested-ternary": 2,
        "no-new": 2,
        "no-new-func": 2,
        "no-new-require": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-sparse-arrays": 2,
        "no-tabs": 2,
        "no-template-curly-in-string": 2,
        "no-undef": 2,
        "no-undef-init": 2,
        "no-undefined": 2,
        "no-unmodified-loop-condition": 2,
        "no-unreachable": 2,
        "no-unused-vars": 2,
        "no-useless-concat": 2,
        "no-useless-rename": 2,
        "no-useless-return": 2,
        "one-var-declaration-per-line": 2,
        "operator-assignment": 2,
        "prefer-const": 2,
        "prefer-destructuring": 2,
        "prefer-object-spread": 2,
        "prefer-rest-params": 2,
        "prefer-spread": 2,
        "prefer-template": 2,
        "use-isnan": 2,
        "valid-typeof": 2,
        "yoda": 2,
        "indent": 2,
        "indent-legacy": 2
    },
    "env": {
        "node": true,
        "browser": true,
        "es6": true
    }
}